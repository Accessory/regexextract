{
	"name": "regexextract",
	"publisher": "regexextract",
	"displayName": "Regex Extract",
	"description": "",
	"repository": {
		"url": "https://github.com/Accessory/regexextract"
	},
	"version": "0.1.5",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:regexextract.renderRegex",
		"onCommand:regexextract.extractRegex",
		"onCommand:regexextract.extractRegexFromGroup"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "regexextract",
			"properties": {
				"regexextract.regexPresets": {
					"type": "array",
					"default": [],
					"description": "Set default Regexes to select",
					"items": {
						"type": "string"
					}
				}
			}
		},
		"commands": [
			{
				"command": "regexextract.renderRegex",
				"title": "Render Regex"
			},
			{
				"command": "regexextract.extractRegex",
				"title": "Extract Regex"
			},
			{
				"command": "regexextract.extractRegexFromGroup",
				"title": "Extract Regex From Group"
			}
		]
	},
	"scripts": {
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "npm run esbuild-base -- --minify",
		"esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
		"esbuild": "npm run esbuild-base -- --sourcemap",
		"esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
		"test-compile": "tsc -p ./",
		"bundle": "vsce package"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "^14.18.20",
		"@types/vscode": "1.66.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@vscode/test-electron": "^2.1.3",
		"esbuild": "^0.14.42",
		"eslint": "^8.16.0",
		"glob": "^7.2.3",
		"mocha": "^9.2.2",
		"typescript": "^4.7.2"
	}
}
